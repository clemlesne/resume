extend ../templates/common/layout

block vars
  - var headerTitle = 'AXA'
  - var headerMainBottom = "Self-taught, entrepreneur, in project management and digital strategy, I work at AXA's global IT department as Lead DevOps for a critical business line. I am open to Architect/Transformation positions to help clients get the most out of their business."
  - var isFooterLeft = false
  - var isQrcodeVisible = false
  - var isPictureHide = true

block header__image
  aside.header__image.header__image--aside
    img(src='img/cert-aws-architect.png?v=@@version')

block content
